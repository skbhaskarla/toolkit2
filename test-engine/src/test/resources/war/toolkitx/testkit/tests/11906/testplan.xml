    <StoredQueryTransaction>
      <XDSb/>
      <MetadataFile>query2.xml</MetadataFile>
      <UseId testdir="../12346" id="SubmissionSet01" symbol="$uniqueid$" step="submit_2doc_w_fol" section="AssignedUids"/>
      <ExpectedContents>
        <SSwithTwoDocOneFol/>
      </ExpectedContents>
    </StoredQueryTransaction>
  </TestStep>
