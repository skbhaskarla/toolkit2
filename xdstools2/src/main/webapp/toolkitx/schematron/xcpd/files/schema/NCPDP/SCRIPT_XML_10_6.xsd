									<xs:restriction base="xs:string">
										<xs:pattern value="P|C|M|D|ZZZ"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="From">
				<xs:annotation>
					<xs:documentation>UIB-060-1</xs:documentation>
				</xs:annotation>
