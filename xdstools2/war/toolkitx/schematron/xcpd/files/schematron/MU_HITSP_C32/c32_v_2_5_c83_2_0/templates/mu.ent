<!--  Meaningful Uuse Required Sections: mu -->



<!-- errors -->

<pattern id='p-mu-errors' see='#p-mu-errors'>
	<title>Meaningful Use Required Sections - errors validation phase</title>
	<rule context='/'>
        <assert test="//cda:templateId[@root='2.16.840.1.113883.3.88.11.32.1']">A Meaningful Use HITSP/C32 v2.5 document SHALL contain the template ID for HITSP/C32 v2.5 (2.16.840.1.113883.3.88.11.32.1).</assert>	
	<assert test="(//cda:templateId[@root='2.16.840.1.113883.3.88.11.83.122']) or 
                      (//cda:section[.//cda:templateId/@root='2.16.840.1.113883.3.88.11.83.17' and
                                     .//cda:templateId/@root='2.16.840.1.113883.3.88.11.83.15.1'])">A Meaningful Use HITSP/C32 v2.5 document SHALL contain a Results Section.</assert>
	<assert test="//cda:templateId[@root='2.16.840.1.113883.3.88.11.83.103'] or
                      //cda:templateId[@root='2.16.840.1.113883.3.88.11.83.7']">A Meaningful Use HITSP/C32 v2.5 document SHALL contain a Problems Section.</assert>
	<assert test="//cda:templateId[@root='2.16.840.1.113883.3.88.11.83.112'] or
                      //cda:templateId[@root='2.16.840.1.113883.3.88.11.83.8']">A Meaningful Use HITSP/C32 v2.5 document SHALL contain a Medications Section.</assert>
	<assert test="//cda:templateId[@root='2.16.840.1.113883.3.88.11.83.102'] or
                      //cda:templateId[@root='2.16.840.1.113883.3.88.11.83.6']">A Meaningful Use HITSP/C32 v2.5 document SHALL contain an Allergies Section.</assert>
	</rule>
</pattern>

