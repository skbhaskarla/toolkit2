	<xs:simpleType name="MailAddressType">
		<xs:restriction base="xs:string">
			<xs:pattern value="(mailto:)?[^\.]{1,35}(\.[^@]{1,3})?(@[A-Za-z0-9]+\.[A-Za-z]{2,4})?"/>
