<ivysettings>
	<settings defaultResolver="shared" />
	
	<credentials host="vm-070.nist.gov" realm="Sonatype Nexus Repository Manager"
		username="deployment" passwd="deployment123" />	
	
	<property name="nexus-public"
		value="http://vm-070.nist.gov:8081/nexus/content/groups/public" />
	<property name="nexus-releases"
		value="http://vm-070.nist.gov:8081/nexus/nexus/content/repositories/releases" />
	<property name="nexus-snapshots"
		value="http://vm-070.nist.gov:8081/nexus/nexus/content/repositories/snapshots" />
		
		
	<resolvers>
		<ibiblio name="shared" m2compatible="true" root="${nexus-public}" />
		<ibiblio name="shared-releases" m2compatible="true"
			root="${nexus-releases}" />
		<ibiblio name="shared-snapshots" m2compatible="true"
			root="${nexus-snapshots}" checkmodified="true" changingPattern="*-SNAPSHOT" />
	</resolvers>
	
</ivysettings>